{
  "name": "veloq",
  "version": "0.1.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanjt/veloq"
  },
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "clean:rust": "npm run clean --prefix modules/veloqrs",
    "rebuild:android": "npm run clean:rust && npx expo run:android",
    "rebuild:ios": "npm run clean:rust && npx expo run:ios",
    "build:info": "cat modules/veloqrs/android/src/main/jniLibs/BUILD_INFO 2>/dev/null || echo 'No build info found'",
    "web": "npx expo start --web",
    "test": "jest --config config/jest.config.js",
    "test:watch": "jest --config config/jest.config.js --watch",
    "test:coverage": "jest --config config/jest.config.js --coverage",
    "test:perf": "jest --config config/jest.config.js --testPathPattern=perf.test --verbose",
    "test:perf:ci": "jest --config config/jest.config.js --testPathPattern=perf.test --json --outputFile=perf-results.json",
    "format": "prettier --config config/.prettierrc --ignore-path config/.prettierignore --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --config config/.prettierrc --ignore-path config/.prettierignore --check \"src/**/*.{ts,tsx}\"",
    "prepare": "husky",
    "postinstall": "node scripts/patch-expo-signing.js",
    "maestro:tier0": "maestro test .maestro/ --include-tags=tier0 --no-ansi",
    "maestro:tier1": "maestro test .maestro/ --include-tags=tier0,tier1 --no-ansi",
    "maestro:tier2": "maestro test .maestro/ --include-tags=tier2 --no-ansi",
    "maestro:tier3": "maestro test .maestro/ --include-tags=tier3 --no-ansi",
    "maestro:regression": "maestro test .maestro/ --include-tags=tier0,tier1,tier2 --no-ansi",
    "maestro:smoke": "maestro test .maestro/ --include-tags=tier0 --no-ansi",
    "maestro:critical": "maestro test .maestro/ --include-tags=tier0,tier1 --no-ansi",
    "maestro:all": "maestro test .maestro/ --include-tags=tier0,tier1,tier2,tier3 --no-ansi",
    "maestro:screenshots": "maestro test .maestro/ --include-tags=tier4 --no-ansi",
    "maestro:stress": "maestro test .maestro/ --include-tags=tier3 --no-ansi",
    "store:changelog": "npx tsx scripts/store-metadata.ts changelog",
    "store:sync": "npx tsx scripts/store-metadata.ts sync-metadata",
    "store:locales": "npx tsx scripts/store-metadata.ts list-locales",
    "ffi:manifest": "npx tsx scripts/extract-ffi-exports.ts",
    "ffi:check": "npx tsx scripts/extract-ffi-exports.ts --check",
    "ffi:test": "jest --config config/jest.config.js --testPathPattern=ffiBinding",
    "ffi:usage": "npx tsx scripts/ffi-usage-report.ts",
    "ffi:unused": "npx tsx scripts/ffi-usage-report.ts --unused"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.3",
    "@mapbox/polyline": "^1.2.1",
    "@maplibre/maplibre-react-native": "^10.4.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@shopify/react-native-skia": "2.2.12",
    "@tanstack/query-async-storage-persister": "^5.90.14",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-persist-client": "^5.90.14",
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.9",
    "expo": "~54.0.30",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.12",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-location": "~19.0.8",
    "expo-network": "~8.0.8",
    "expo-router": "~6.0.21",
    "expo-screen-orientation": "~9.0.8",
    "expo-secure-store": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "i18next": "^25.7.3",
    "intl-pluralrules": "^2.0.1",
    "jszip": "^3.10.1",
    "react": "19.1.0",
    "react-i18next": "^16.5.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "veloqrs": "./modules/veloqrs",
    "victory-native": "^41.20.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-circus": "^30.2.0",
    "jest-expo": "~54.0.2",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "uniffi-bindgen-react-native": "^0.29.3-1"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --config config/.prettierrc --ignore-path config/.prettierignore --write"
    ]
  }
}
