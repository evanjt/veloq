{
  "expo": {
    "name": "Veloq",
    "slug": "veloq",
    "version": "0.0.2",
    "scheme": "veloq",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#5B9BD5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.veloq.app",
      "buildNumber": "2",
      "infoPlist": {
        "CFBundleDevelopmentRegion": "en-AU",
        "CFBundleLocalizations": ["en-AU", "en-US", "en-GB", "es", "fr"],
        "NSLocationWhenInUseUsageDescription": "Veloq uses your location to display activity maps and track your routes.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Veloq uses your location to display activity maps and track your routes.",
        "UIBackgroundModes": ["location", "fetch"],
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:intervals.icu"]
    },
    "android": {
      "package": "com.veloq.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5B9BD5"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "INTERNET",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "./plugins/with-gradle-repositories",
      "./plugins/with-release-signing",
      "./plugins/with-packaging-options",
      "expo-router",
      "expo-localization",
      "@maplibre/maplibre-react-native",
      "expo-web-browser",
      "expo-secure-store",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {},
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
